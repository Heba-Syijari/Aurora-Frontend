{
  "name": "spreadislam-builder",
  "author": "Misraj",
  "version": "1.0.0",
  "description": "CanvaBlocks website builder",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8084",
    "start": "next start -p 8084",
    "build": "next build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "codegen": "graphql-codegen --config codegen.ts && eslint src/graphql --ext .js,.ts --fix",
    "clear-all": "rm -rf node_modules .next out dist build",
    "re-start": "rm -rf node_modules .next out dist build && yarn install && yarn dev",
    "re-build": "rm -rf node_modules .next out dist build && yarn install && yarn build"
  },
